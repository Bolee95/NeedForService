<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!-- The main content view -->
    <FrameLayout
        android:id="@+id/content_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <android.support.v7.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/lista_vozila_na_servisu_recycle_view_id2"
            />
    </FrameLayout>
    <android.support.design.widget.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:fitsSystemWindows="true">
        <!--   app:headerLayout="@layout/nav_header_main"> -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <include layout="@layout/nav_header_main_servis"></include>
            <include layout="@layout/nav_list_main_servis"></include>
        </LinearLayout>
        <!-- app:actionLayout="@layout/nav_list_main" -->

        <!-- JBG -->

        <!--   app:menu="@layout/nav_list_main" -->

    </android.support.design.widget.NavigationView>



</android.support.v4.widget.DrawerLayout>
