<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="Base.Theme.AppCompat">

        <!-- Customize your theme here. -->

        <item name="colorPrimary">@color/Black</item>
        <item name="colorPrimaryDark">@color/Black</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:colorAccent">@color/accent_material_dark</item>
        <item name="android:colorPrimary">@color/primary_material_dark</item>
        <item name="android:navigationBarColor">@color/primary_material_dark</item>
        <item name="android:colorPrimaryDark">@color/primary_material_dark</item>
        <item name="android:colorBackground">@color/background_material_dark</item>
    </style>

    <style name="Green" parent="Theme.AppCompat.NoActionBar">
        <item name="android:colorPrimary">@color/holo_primary</item>
        <item name="colorPrimaryDark">@color/primary_material_dark</item>
        <item name="android:navigationBarColor">@color/primary_material_dark</item>
        <item name="android:colorBackground">@color/background_material_dark</item>
    </style>
</resources>